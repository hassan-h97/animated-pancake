Private Sub MainProg3()

Dim sht As Worksheet

Set sht = Worksheets("Temp")

Set rep = sht.Range("A1:A50").Find(what:=Order_Userform2.Controls("Label_" & LB), LookIn:=xlValues)

If rep Is Nothing Then
    rep1 = 1 'it means that it has not found the text in the range A1:B4
    quan = 0
Else
    rep1 = 2 'it means that it has found the text in the range A1:B4
    row1 = rep.row ' this is the row where the data is contained
    quan = Order_Userform2.ListBox1.Column(3, (row1 - 1))
If quan > 0 Then
    quan = quan - 1
    sht.Cells(row1, 4).Value = quan
    sht.Cells(row1, 6).Value = Order_Userform2.Controls("Label_" & LB & "_" & LB) * sht.Cells(row1, 4).Value
    If quan = 0 Then
        sht.Rows(row1 & ":" & row1).Delete
    End If
End If

End If

'the quantity column is the fourth column across


Call UpdatingSum


End Sub

